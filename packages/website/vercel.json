{
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.gg/MrQdmzd",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/RSC/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "regions": ["sfo1"],
  "trailingSlash": false
}
